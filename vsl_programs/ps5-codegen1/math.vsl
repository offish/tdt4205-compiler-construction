func main(num) {
    print "num = ", num
    print "num*3 = ", num*3
    print "num+5 = ", num+5
    print "num/5 = ", num/5
    print "4*-num = ", 4*-num
    print "(num-1)*(num+1)*(num+3) = ", (num-1)*(num+1)*(num+3)
}

